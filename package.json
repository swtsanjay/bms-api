{
  "name": "ts-node-api-template",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "build": "tsc",
    "watch-ts": "npx tsc -w",
    "dev": "nodemon dist/app.js",
    "start": "node dist/app.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "migrate:latest": "knex migrate:latest --knexfile dist/config/knex.js",
    "migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "migrate:make": "knex migrate:make --knexfile src/config/knex.ts"
  },
  "scriptComments": {
    "start": "In this the .env variable is going to be set by docker"
