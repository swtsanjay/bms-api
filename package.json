    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.2",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
